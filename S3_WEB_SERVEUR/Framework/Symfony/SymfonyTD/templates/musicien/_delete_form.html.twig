<form method="post" action="{{ path('musicien_delete', {'codeMusicien': musicien.codeMusicien}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ musicien.codeMusicien) }}">
    <button class="btn">Delete</button>
</form>
